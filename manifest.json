{
    "manifest_version": 3,
    "name": "Timer Extension",
    "version": "1.0",
    "description": "A simple timer extension.",
    "permissions": ["notifications"],
    "background":{
        "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  